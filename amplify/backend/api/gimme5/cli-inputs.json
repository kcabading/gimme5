{
  "version": 1,
  "paths": {
    "/questions": {
      "name": "/questions",
      "lambdaFunction": "gimme5Questions",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/users": {
      "name": "/users",
      "lambdaFunction": "gimme5UserProfiles",
      "permissions": {
        "setting": "open"
      }
    },
    "/games": {
      "name": "/games",
      "lambdaFunction": "gimme5Games",
      "permissions": {
        "setting": "open"
      }
    }
  }
}